{
  "expo": {
    "name": "jalaudang",
    "slug": "jalaudang",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/img-logo.png",
    "entryPoint": "./src/App.js",
    "splash": {
      "image": "./src/assets/images/img-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#EAEFF5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.jalaudang"
    },
    "web": {
      "favicon": "./src/assets/images/img-favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
